{
  "name": "thirteen-angular",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "build": "ng build",
    "build-prod": "ng build --prod --configuration=production --aot --build-optimizer",
    "build-prod-base": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --prod --base-href ./ --configuration=production --aot --build-optimizer",
    "build-test": "ng build --prod --configuration=test --build-optimizer",
    "build-test-base": "ng build --base-href ./ --prod --configuration=test --build-optimizer",
    "build-stats-json": "ng build --prod --configuration=production --build-optimizer --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/thirteen-angular/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.4",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/http": "~7.2.16",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/router": "~9.1.4",
    "@angular/upgrade": "~9.1.4",
    "angular-in-memory-web-api": "~0.11.0",
    "core-js": "~3.6.5",
    "echarts": "~4.8.0",
    "ng-zorro-antd": "~9.2.2",
    "ngx-echarts": "^4.1.1",
    "rxjs": "^6.6.0",
    "tslib": "^1.13.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/language-service": "~9.1.4",
    "@types/echarts": "~4.6.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "~6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lodash": "^4.16.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "~3.8.0",
    "node-sass": "^4.14.1"
  }
}
