{
  "name": "thirteen-angular",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "build": "ng build",
    "build-prod": "ng build --prod --configuration=prod --build-optimizer",
    "build-prod-base": "ng build --base-href ./ --prod --aot --configuration=prod --build-optimizer",
    "build-test": "ng build --prod --configuration=test --build-optimizer",
    "build-test-base": "ng build --base-href ./ --prod --configuration=test --build-optimizer",
    "build-stats-json": "ng build --prod --configuration=prod --build-optimizer --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/angular6/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.7",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/forms": "^7.2.7",
    "@angular/http": "^7.2.7",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "echarts": "^4.2.1",
    "ngx-echarts": "^4.1.1",
    "core-js": "^2.6.5",
    "ng-zorro-antd": "^7.0.3",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
